{
  "mcpServers": {
    "binance-futures-mcp": {
      "command": "uv",
      "args": [
        "--directory",
        "/root/binance-mcp-server",
        "run",
        "-m",
        "binance_mcp.server",
        "--binance-api-key",
        "YOUR_API_KEY",
        "--binance-secret-key",
        "YOUR_SECRET_KEY"
      ]
    }
  }
}
